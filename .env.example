APP_NAME=ServiceDesk
APP_ENV=local
APP_KEY=SECRET:GENERATE
APP_DEBUG=SAFE_DEFAULT=false
APP_URL=SAFE_DEFAULT=http://localhost

LOG_CHANNEL=SAFE_DEFAULT=stack
LOG_LEVEL=SAFE_DEFAULT=debug
LOG_STACK_CHANNELS=SAFE_DEFAULT=daily
LOG_STDERR_FORMATTER=SAFE_DEFAULT=null

DB_CONNECTION=SAFE_DEFAULT=mysql
DB_HOST=SAFE_DEFAULT=db
DB_PORT=SAFE_DEFAULT=3306
DB_DATABASE=SAFE_DEFAULT=servicedesk
DB_USERNAME=SECRET:db_username
DB_PASSWORD=SECRET:db_password

DB_PG_CONNECTION=SAFE_DEFAULT=pgsql
DB_PG_HOST=SAFE_DEFAULT=db
DB_PG_PORT=SAFE_DEFAULT=5432
DB_PG_DATABASE=SAFE_DEFAULT=servicedesk
DB_PG_USERNAME=SECRET:pg_username
DB_PG_PASSWORD=SECRET:pg_password

BROADCAST_DRIVER=SAFE_DEFAULT=log
CACHE_DRIVER=SAFE_DEFAULT=redis-fallback
CACHE_REDIS_CONNECTION=SAFE_DEFAULT=cache
CACHE_FALLBACK_STORE=SAFE_DEFAULT=file
FILESYSTEM_DISK=SAFE_DEFAULT=public
QUEUE_CONNECTION=SAFE_DEFAULT=redis
SESSION_DRIVER=SAFE_DEFAULT=redis-fallback
SESSION_STORE=SAFE_DEFAULT=redis-fallback
SESSION_CONNECTION=SAFE_DEFAULT=cache
SESSION_LIFETIME=SAFE_DEFAULT=120

REDIS_HOST=SAFE_DEFAULT=redis
REDIS_PASSWORD=SECRET:redis_password
REDIS_PORT=SAFE_DEFAULT=6379

MEILISEARCH_HOST=SAFE_DEFAULT=http://meilisearch:7700
MEILISEARCH_KEY=SECRET:meili_master_key
MEILISEARCH_HEALTHCHECK_URL=SAFE_DEFAULT=http://meilisearch:7700/health
MEILISEARCH_HEALTHCHECK_TIMEOUT=SAFE_DEFAULT=2
MEILISEARCH_BACKUP_PATH=SAFE_DEFAULT=storage/app/backups/meilisearch
MEILISEARCH_BACKUP_RETENTION_DAYS=SAFE_DEFAULT=14
MEILISEARCH_KB_ARTICLES_INDEX=SAFE_DEFAULT=kb_articles
MEILISEARCH_CORRELATION_PREFIX=SAFE_DEFAULT=meilisearch

MAIL_MAILER=SAFE_DEFAULT=log
MAIL_HOST=SAFE_DEFAULT=mailpit
MAIL_PORT=SAFE_DEFAULT=1025
MAIL_USERNAME=SECRET:mail_username
MAIL_PASSWORD=SECRET:mail_password
MAIL_ENCRYPTION=SAFE_DEFAULT=null
MAIL_FROM_ADDRESS=SAFE_DEFAULT=helpdesk@example.com
MAIL_FROM_NAME=SAFE_DEFAULT="Service Desk"

SCOUT_DRIVER=SAFE_DEFAULT=meilisearch
SCOUT_QUEUE=SAFE_DEFAULT=true

SANCTUM_STATEFUL_DOMAINS=SAFE_DEFAULT=localhost
SESSION_DOMAIN=SAFE_DEFAULT=.localhost

PUSHER_APP_ID=SECRET:pusher_app_id
PUSHER_APP_KEY=SECRET:pusher_app_key
PUSHER_APP_SECRET=SECRET:pusher_app_secret
PUSHER_HOST=SAFE_DEFAULT=127.0.0.1
PUSHER_PORT=SAFE_DEFAULT=6001
PUSHER_SCHEME=SAFE_DEFAULT=http
PUSHER_APP_CLUSTER=SAFE_DEFAULT=mt1
PUSHER_FORCE_TLS=SAFE_DEFAULT=false
PUSHER_ENCRYPTED=SAFE_DEFAULT=true
PUSHER_CLIENT_TIMEOUT=SAFE_DEFAULT=5
ECHO_ENABLED=SAFE_DEFAULT=false

FILAMENT_PATH=SAFE_DEFAULT=/admin
FILAMENT_BRAND=SAFE_DEFAULT=Service Desk

QUEUE_SUPERVISOR_CONNECTION=SAFE_DEFAULT=redis
QUEUE_SUPERVISOR_QUEUE=SAFE_DEFAULT=default

# REQUIRED: Set APP_KEY via artisan key:generate before production.
# OPTIONAL: Override *_HOST to point at managed services.
# DO NOT COMMIT: Any file named .env
