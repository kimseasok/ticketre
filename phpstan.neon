includes:
    - vendor/nunomaduro/larastan/extension.neon
    - phpstan-baseline.neon

parameters:
    paths:
        - app
        - bootstrap
        - config
        - database
        - routes
        - tests
    level: 8
    checkGenericClassInNonGenericObjectType: false
    excludePaths:
        - database/migrations
        - storage
