{
    "name": "kimseasok/prompt-engineering",
    "description": "Prompt engineering service desk skeleton",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.5",
        "laravel/scout": "^10.6",
        "laravel/sail": "^1.30",
        "laravel/sanctum": "^4.0",
        "spatie/laravel-permission": "^6.11",
        "filament/filament": "^3.2",
        "meilisearch/meilisearch-php": "^1.7",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6",
        "nunomaduro/larastan": "^2.9",
        "pestphp/pest": "^3.4",
        "pestphp/pest-plugin-laravel": "^3.0",
        "spatie/laravel-ray": "^1.33"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
